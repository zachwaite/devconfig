#! /usr/bin/env bash

set -o errexit
set -o xtrace
set -o nounset

# shellcheck source=/dev/null
source "$DEVCONFIG_ROOT/bin/devconfig.sh"

thaw_vimrc
thaw_tmuxconf
thaw_snippets
thaw_initvimrc

cat << EOF
Remember this DOES NOT thaw .bashrc!
Remember to add this to your bashrc:

# devconfig
export DEVCONFIG_ROOT=/home/zach/VimProjects/devconfig
export PATH="\$PATH:/home/zach/VimProjects/devconfig/bin"
EOF
